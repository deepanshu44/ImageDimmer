{
  "manifest_version": 2,
  "version": "1.0",
  "name": "ImageDimmer",
  "description": "decrease brightness of image",
  "icons": {
    "64": "ImageDimmer_icon.jpg"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content_script.js"]
    }
  ],
  "browser_action": {
    "default_icon": {
      "64": "ImageDimmer_icon.jpg"
    },
    "default_popup": "popup.html",
    "default_title": "ImageDimmer"
  },
  "permissions": [ "storage" ]
}
